{
  "name": "file-explorer",
  "productName": "file-explorer",
  "version": "0.1.0",
  "description": "File Explorer",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "tsc-watch": "tsc --watch --noEmit",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --max-warnings 0 .",
    "storybook": "concurrently \"start-storybook -p 6006 -s ./src/static\" \"ts-node --project ./tsconfig.json -r tsconfig-paths/register ./.storybook/file-icon-theme-server.ts\""
  },
  "keywords": [],
  "author": {
    "name": "Patrick Kerschbaum",
    "email": "patrick.kerschbaum@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": "^14.18.0",
    "npm": "^6.14.15",
    "yarn": "^1.22.15"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --max-warnings 0"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.61",
    "@storybook/addon-actions": "^6.3.11",
    "@storybook/addon-essentials": "^6.3.11",
    "@storybook/addon-links": "^6.3.11",
    "@storybook/react": "^6.3.11",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "axios": "^0.23.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.3",
    "concurrently": "^6.3.0",
    "copy-webpack-plugin": "^9.0.1",
    "cors": "^2.8.5",
    "css-loader": "^6.0.0",
    "electron": "^14.2.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "node-loader": "^2.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3",
    "typescript-plugin-styled-components": "^2.0.0",
    "typescript-styled-plugin": "^0.18.1"
  },
  "dependencies": {
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.6",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^5.0.4",
    "@pkerschbaum/code-oss-file-icon-theme": "^2.0.0",
    "@pkerschbaum/code-oss-file-service": "^1.61.1",
    "@reduxjs/toolkit": "^1.6.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "match-sorter": "^6.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.29.0",
    "react-redux": "^7.2.5",
    "scheduler": "^0.20.2",
    "styled-components": "^5.3.3",
    "use-context-selector": "^1.3.9",
    "use-immer": "^0.6.0"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@^5.0.4"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_react_typescript"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  }
}
