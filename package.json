{
  "name": "pkerschbaum-homepage",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "w:nuke": "pnpm run --parallel pkg:nuke && pnpm run w:nuke:node-modules",
    "w:nuke:artifacts": "pnpm run --parallel pkg:nuke:artifacts",
    "w:nuke:node-modules": "pnpm --parallel exec rimraf node_modules && rimraf node_modules",
    "w:lint": "pnpm run --parallel pkg:lint",
    "w:lint:fix": "pnpm run --parallel pkg:lint:fix",
    "w:format": "prettier --write --ignore-unknown .",
    "w:build": "pnpm --recursive run pkg:build",
    "w:compile:dry-run": "pnpm --recursive run pkg:compile:dry-run"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2"
  },
  "pnpm": {
    "overrides": {
      "@mui/styled-engine": "npm:@mui/styled-engine-sc@^5.10.6"
    }
  },
  "packageManager": "pnpm@7.11.0",
  "engines": {
    "pnpm": "^7.11.0"
  }
}
