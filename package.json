{
  "name": "file-explorer",
  "productName": "file-explorer",
  "version": "0.1.0",
  "description": "File Explorer",
  "main": ".webpack/main",
  "scripts": {
    "postinstall": "node ./scripts/remove-cypress-globals.js && node ./scripts/modify-tsserver-to-defer-type-truncation.js",
    "start": "cross-env NODE_ENV=development cross-env ELECTRONAPP_RQDEVTOOLS_ENABLED=false electron-forge start",
    "tsc-watch": "tsc --watch --noEmit",
    "package": "cross-env ELECTRONAPP_RQDEVTOOLS_ENABLED=false electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --max-warnings 0 .",
    "test": "jest --no-cache",
    "test:watch": "jest --no-cache --runInBand --watch",
    "cypress:run": "cross-env ELECTRON_EXTRA_LAUNCH_ARGS=\"--force-prefers-reduced-motion\" cypress run --browser chrome",
    "cypress:run:update": "cross-env ELECTRON_EXTRA_LAUNCH_ARGS=\"--force-prefers-reduced-motion\" cypress run --browser chrome --env updateSnapshots=true",
    "cypress:open:update": "cross-env ELECTRON_EXTRA_LAUNCH_ARGS=\"--force-prefers-reduced-motion\" cypress open --browser chrome --env updateSnapshots=true",
    "storybook": "cross-env STORYBOOK_RQDEVTOOLS_ENABLED=false start-storybook -c storybook -p 6006",
    "build-storybook": "cross-env STORYBOOK_RQDEVTOOLS_ENABLED=false build-storybook -c storybook",
    "serve-static-storybook": "yarn run http-server -p 6006 ./storybook-static",
    "compile-language-extensions-into-json-file": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./scripts/compile-language-extensions-into-json-file.ts && prettier --write ./src/static/icon-theme/language-extension-points.json"
  },
  "keywords": [],
  "author": {
    "name": "Patrick Kerschbaum",
    "email": "patrick.kerschbaum@gmail.com"
  },
  "license": "MIT",
