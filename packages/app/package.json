{
  "name": "@file-explorer/app",
  "productName": "file-explorer",
  "version": "0.1.0",
  "description": "File Explorer",
  "main": ".webpack/main",
  "scripts": {
    "prepare": "node ./scripts/modify-tsserver-to-defer-type-truncation.js",
    "nuke": "pnpm run pkg:nuke:artifacts && rimraf node_modules",
    "nuke:artifacts": "rimraf .webpack out storybook-static test-results",
    "start": "cross-env NODE_ENV=development cross-env ELECTRONAPP_RQDEVTOOLS_ENABLED=false electron-forge start",
    "compile:dry-run": "tsc --noEmit",
    "compile:dry-run:watch": "pnpm run compile:dry-run --watch",
    "package": "cross-env ELECTRONAPP_RQDEVTOOLS_ENABLED=false electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --max-warnings 0",
    "test:unit": "jest --no-cache",
    "test:unit:watch": "jest --no-cache --runInBand --watch",
    "pretest:pw": "playwright install",
    "test:pw": "playwright test --config=playwright/playwright.config.ts",
    "test:pw:update": "pnpm run test:pw --update-snapshots",
    "test:pw-docker": "docker compose -f ./playwright/docker-compose.yml build playwright-service && docker compose -f ./playwright/docker-compose.yml run --rm playwright-service",
    "test:pw-docker:update": "pnpm run test:pw-docker --update-snapshots",
    "storybook": "cross-env STORYBOOK_RQDEVTOOLS_ENABLED=false start-storybook -c storybook -p 6006",
    "build-storybook": "cross-env STORYBOOK_RQDEVTOOLS_ENABLED=false build-storybook -c storybook",
    "serve-static-storybook": "pnpm run http-server -p 6006 ./storybook-static",
    "build-and-serve-storybook": "pnpm run build-storybook && pnpm run serve-static-storybook",
    "compile-language-extensions-into-json-file": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./scripts/compile-language-extensions-into-json-file.ts && prettier --write ./src/static/icon-theme/language-extension-points.json",
    "pkg:nuke": "pnpm run nuke",
    "pkg:nuke:artifacts": "pnpm run nuke:artifacts",
    "pkg:lint": "pnpm run lint .",
    "pkg:lint:fix": "pnpm run lint . --fix",
    "pkg:lint:file": "pnpm run lint",
    "pkg:build": "pnpm run package",
    "pkg:compile:dry-run": "pnpm run compile:dry-run"
  },
  "keywords": [],
  "author": {
    "name": "Patrick Kerschbaum",
    "email": "patrick.kerschbaum@gmail.com"
