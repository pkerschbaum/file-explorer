{
  "name": "@file-explorer/web",
  "version": "0.0.1",
  "description": "File Explorer",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "Patrick Kerschbaum",
    "email": "patrick.kerschbaum@gmail.com"
  },
  "main": ".webpack/main",
  "scripts": {
    "build": "pnpm run internal:compile",
    "build-and-serve-storybook": "pnpm run storybook build && pnpm run serve-static-storybook",
    "build-storybook": "cross-env STORYBOOK_RQDEVTOOLS_ENABLED=false storybook build -c storybook",
    "build:next": "pnpm run nuke:artifacts && pnpm run internal:compile && pnpm run internal:build:next",
    "dev": "pnpm run internal:compile --watch --preserveWatchOutput",
    "dev:next": "next dev",
    "internal:build:next": "next build",
    "internal:compile": "tsc -p ./tsconfig.project.json",
    "lint": "pnpm run lint:eslint",
    "lint:eslint": "pnpm run lint:file .",
    "lint:file": "eslint --max-warnings 0",
    "lint:fix": "pnpm run lint:file . --fix",
    "nuke": "pnpm run nuke:artifacts && del-cli node_modules",
    "nuke:artifacts": "del-cli dist \".next\" \"*.tsbuildinfo\"",
    "prepare": "ts-patch install -s",
    "serve-static-storybook": "pnpm run http-server -p 6006 ./storybook-static",
    "start": "next start",
    "storybook": "cross-env STORYBOOK_RQDEVTOOLS_ENABLED=false storybook dev -c storybook -p 6006",
    "test:pw": "playwright install chromium && playwright test --config=playwright/playwright.config.ts",
    "test:pw-docker": "docker compose -f ./playwright/docker-compose.yml build playwright-service && docker compose -f ./playwright/docker-compose.yml run --rm playwright-service",
    "test:pw-docker:update": "pnpm run test:pw-docker --update-snapshots",
    "test:pw:update": "pnpm run test:pw --update-snapshots",
    "test:unit": "jest --no-cache",
    "test:unit:watch": "jest --no-cache --runInBand --watch"
  },
  "dependencies": {
    "@file-explorer/agent": "workspace:*",
    "@file-explorer/code-oss-ecma": "workspace:*",
    "@file-explorer/commons-ecma": "workspace:*",
